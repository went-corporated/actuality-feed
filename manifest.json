{
    "manifest_version": 3,
    "name": "Actuality Feed",
    "version": "1.0.0.0",
    "version_name": "1.0.0",
    "description": "Outil de communication simple et pratique pour les membres de l'organisation Went Lab.",
    "author": "Went Laboratory",
    "permissions": [
        "webRequest",
        "storage",
        "unlimitedStorage"
    ],
    "icons": {
        "16": "./icons/news-16.png",
        "48": "./icons/news-48.png",
        "128": "./icons/news-128.png"
    },
    "action": {
        "default_popup": "./popup/popup.html",
        "default_title": "Actuality Feed"
    },
    "background": {
        "service_worker": "./background.js"
    }
}