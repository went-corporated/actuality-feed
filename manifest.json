{
    "manifest_version": 3,
    "name": "Actuality Feed",
    "version": "1.0.0.0",
    "description": "Outil de communication simple et pratique pour les membres de l'organisation Went Laboratory.",
    "author": "Went Laboratory",
    "permissions": [
        "activeTab",
        "tabs",
        "webNavigation",
        "webRequest",
        "storage",
        "unlimitedStorage",
        "storage"
    ],
    "icons": {
        "16": "./icons/a.png",
        "48": "./icons/b.png"
    },
    "action": {
        "default_popup": "./popup/popup.html",
        "default_title": "Actuality Feed"
    },
    "background": {
        "service_worker": "./background.js"
    }
}